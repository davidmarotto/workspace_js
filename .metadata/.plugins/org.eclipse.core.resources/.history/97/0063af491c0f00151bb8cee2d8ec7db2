var fs = require('fs');

var filename = process.argv[2];

try {
	var buf = fs.readFileSync(filename);
	//console.log(buf.toString());
	var chunks = buf.toString().split(' ');
	console.log(chunks.length);
} catch(err) {
	console.log("Err: " + err);
}

